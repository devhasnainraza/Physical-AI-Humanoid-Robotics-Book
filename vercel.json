{
  "version": 2,
  "builds": [
    {
      "src": "api/py/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/py/(.*)",
      "dest": "/api/py/index.py"
    },
    {
      "src": "/",
      "dest": "/api/py/"
    }
  ],
  "buildCommand": "echo 'Skipping frontend build for backend-only deployment'",
  "outputDirectory": "."
}